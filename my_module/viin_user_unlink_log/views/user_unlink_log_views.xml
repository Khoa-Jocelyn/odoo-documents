<odoo>
	<data>
		<record model="ir.ui.view" id="user_unlink_log_view_tree">
			<field name="name">user.unlink.log.tree</field>
			<field name="model">user.unlink.log</field>
			<field name="arch" type="xml">
				<tree string="Unlink Logs">
					<field name="datetime" />
					<field name="model_id" />
					<field name="res_model" />
					<field name="res_id" />
					<field name="res_name" />
					<field name="record_infor" />
					<field name="user_id" />
					<field name="company_id" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="user_assignment_view_search">
			<field name="name">user.assignment.search</field>
			<field name="model">user.assignment</field>
			<field name="arch" type="xml">
				<search string="Search User Assignment">
					<field name="model_id" />
					<field name="res_name" />
					<separator />
					<field name="date_start" />
					<field name="date_end" />
					<separator />
					<field name="user_id" />
					<separator />
					<field name="assigned_by_user_id" />
					<separator groups="base.group_multi_company" />
					<field name="company_id" groups="base.group_multi_company" />
					<group expand="0" string="Group By">
						<filter string="Model" name="grp_model_id" domain="[]"
							context="{'group_by':'model_id'}" />
						<separator />
						<filter string="User" name="grp_user_id" domain="[]"
							context="{'group_by':'user_id'}" />
						<separator />
						<filter string="Assigned By" name="grp_assigned_by_user_id"
							domain="[]" context="{'group_by':'assigned_by_user_id'}" />
						<separator />
						<filter string="Assigned 
			Date" name="grp_date_start"
							domain="[]" context="{'group_by':'date_start'}" />
						<filter string="Unassigned Date" name="grp_date_end"
							domain="[]" context="{'group_by':'date_end'}" />
						<separator />
						<filter string="Company" name="grp_company_id" domain="[]"
							context="{'group_by':'company_id'}"
							groups="base.group_multi_company" />
					</group>
				</search>
			</field>
		</record>

		<record model="ir.actions.act_window"
			id="action_user_unlink_log">
			<field name="name">User Unlink Log</field>
			<field name="res_model">user.unlink.log</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem name="User Unlink" sequence="2"
			id="menu_user_unlink_log_root" parent="base.menu_custom" />

		<menuitem name="Log" id="menu_user_unlink_log"
			parent="menu_user_unlink_log_root" action="action_user_unlink_log" />

	</data>
</odoo>